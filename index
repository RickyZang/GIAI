<span class="k">def</span> <span class="nf">process_llm_response</span><span class="p">(</span><span class="n">llm_response</span><span class="p">):</span>
    <span class="nb">print</span><span class="p">(</span><span class="n">llm_response</span><span class="p">[</span><span class="s1">'result'</span><span class="p">])</span>
    <span class="nb">print</span><span class="p">(</span><span class="s1">'</span><span class="se">\n\n</span><span class="s1">Sources:'</span><span class="p">)</span>
    <span class="k">for</span> <span class="n">source</span> <span class="ow">in</span> <span class="n">llm_response</span><span class="p">[</span><span class="s2">"source_documents"</span><span class="p">]:</span>
        <span class="nb">print</span><span class="p">(</span><span class="n">source</span><span class="o">.</span><span class="n">metadata</span><span class="p">[</span><span class="s1">'source'</span><span class="p">])</span>
